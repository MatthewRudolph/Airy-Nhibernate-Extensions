<package >
  <metadata>
    <id>$id$</id>
    <version>$version$</version>
    <title>$title$</title>
    <authors>$author$</authors>
    <owners>$author$</owners>
    <licenseUrl>https://github.com/MatthewRudolph/Airy-Nhibernate-Extensions/blob/master/LICENSE</licenseUrl>
